<div>
  <% PreferenceCategory.all.each do |instance| %>
    <span class="label label-default chip"><%= instance.category.name + " " %>
      <%= link_to category_preference_category_path(instance.category_id, instance.id), method: :delete do %>
        <i class="fas fa-times"></i>
      <% end %>
    </span>
  <% end %>
</div>
